    "version": "0.2.0",
    "configurations": [
        {
            "name": "stopsctl update",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/stopsctl",
            "envFile": "${workspaceFolder}/.env.dev",
            "args": [
                "update"
            ]
        },
        {
            "name": "stopsctl score",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/stopsctl",
            "envFile": "${workspaceFolder}/.env.dev",
            "args": [
                "score",
                "-k", 
                "localhost:8080",
            ]
        },
        {
            "name": "ttssctl",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/ttssctl",
            "args": [
                "deps", "--bus", "http://localhost:8080", "--tram", "http://localhost:8070"
            ]
        },
        {
            "name": "mock",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/mock"
        },
        {
            "name": "krkstops",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/krkstops",
            "envFile": "${workspaceFolder}/.env.dev"
        },

    ]
}